{
  "name": "yolo-annotator",
  "displayName": "YOLO Annotator",
  "description": "Visual annotation tool for YOLO object detection datasets",
  "version": "1.0.5",
  "publisher": "TanvirTaaha",
  "author": {
    "name": "Tanvir Hossain Taaha",
    "email": "tanvir.taaha@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TanvirTaaha/yolo-annotator-vscode"
  },
  "bugs": {
    "url": "https://github.com/TanvirTaaha/yolo-annotator-vscode/issues"
  },
  "homepage": "https://github.com/TanvirTaaha/yolo-annotator-vscode#readme",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Machine Learning",
    "Data Science",
    "Other"
  ],
  "keywords": [
    "yolo",
    "annotation",
    "object-detection",
    "machine-learning",
    "computer-vision",
    "bounding-box",
    "dataset",
    "labeling",
    "ultralytics"
  ],
  "icon": "assets/icon128.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "license": "MIT",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "yolo-annotator.start",
        "title": "Open YOLO Annotator",
        "category": "YOLO Annotator",
        "icon": "$(pencil)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "yolo-annotator.start",
          "group": "navigation",
          "when": "explorerResourceIsFolder"
        }
      ],
      "commandPalette": [
        {
          "command": "yolo-annotator.start",
          "title": "YOLO Annotator: Open Setup Window"
        }
      ]
    },
    "keybindings": [
      {
        "command": "yolo-annotator.start",
        "key": "ctrl+shift+y",
        "mac": "cmd+shift+y"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "debug-buffer": "node --trace-deprecation --trace-warnings npm run compile"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  }
}
